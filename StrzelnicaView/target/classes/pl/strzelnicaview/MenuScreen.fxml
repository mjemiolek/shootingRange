<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.strzelnicaview.MenuScreenController">
   <children>
      <Label alignment="TOP_CENTER" prefHeight="54.0" prefWidth="418.0" text="Shooting Range Manager" textAlignment="CENTER" textOverrun="CLIP">
         <font>
            <Font size="36.0" />
         </font></Label>
      <Pane prefHeight="69.0" prefWidth="760.0" />
      <Button onAction="#GoToAddReservation" prefHeight="33.0" prefWidth="190.0" text="Add reservation">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button onAction="#GoToShowReservation" prefHeight="33.0" prefWidth="190.0" text="Show reservations">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button onAction="#GoToClientMenu" prefHeight="33.0" prefWidth="190.0" text="Client Menu">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button onAction="#GoToWorkerMenu" prefHeight="33.0" prefWidth="190.0" text="Worker Menu">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button onAction="#GoToWeaponMenu" prefHeight="33.0" prefWidth="190.0" text="Add Weapons">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#GoToWeaponShow" prefHeight="33.0" prefWidth="190.0" text="Show/Delete Weapons">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#Exit" prefHeight="33.0" prefWidth="190.0" text="Exit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
